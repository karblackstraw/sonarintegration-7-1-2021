pipeline {
    agent any
    stages {
        stage("Code Checkout from Gihub") {
            steps {
                git branch: 'master',
                credentialsId: 'Github-Access',
                url: 'https://github.com/karblackstraw/sonarintegration-7-1-2021.git'
            }
        }
        stage('Code Quality Check via SonarQube') {
            steps {
                script {
                    scannerHome = tool 'SonarQube Scanner 4.6.2';
                    withSonarQubeEnv("SonarQube-Server") {
                    sh "${tool("scannerHome")}/bin/sonar-scanner \
                    -Dsonar.projectKey=jenkins-pipeline101 \
                    -Dsonar.sources=. \
                    -Dsonar.host.url=http://3.228.219.245:9000 \
                    -Dsonar.login=SonarQube-Server-2"
                    }
                }
            }
        }
    }
}
        